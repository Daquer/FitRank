<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>FitRank</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>RecuperaFitness</servlet-name>
    <servlet-class>br.com.fitrank.controller.RecuperaFitness</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RecuperaFitness</servlet-name>
    <url-pattern>/RecuperaFitness</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>RecuperaUsuarioFacebook</servlet-name>
    <servlet-class>br.com.fitrank.controller.RecuperaUsuarioFacebook</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RecuperaUsuarioFacebook</servlet-name>
    <url-pattern>/RecuperaUsuarioFacebook</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>InitUser</servlet-name>
    <servlet-class>br.com.fitrank.controller.InitUser</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>InitUser</servlet-name>
    <url-pattern>/InitUser</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>CarregaEscolhaRanking</servlet-name>
    <servlet-class>br.com.fitrank.controller.CarregaEscolhaRanking</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CarregaEscolhaRanking</servlet-name>
    <url-pattern>/CarregaEscolhaRanking</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>SalvaConfiguracao</servlet-name>
    <servlet-class>br.com.fitrank.controller.SalvaConfiguracao</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SalvaConfiguracao</servlet-name>
    <url-pattern>/SalvaConfiguracao</url-pattern>
  </servlet-mapping>
</web-app>